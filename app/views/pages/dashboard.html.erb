
<div class="container">
  <h1 class="text-center">My favorite recipes</h1>
  <div class="row">
    <div class="col-xs-12 text-center">
      <% @favorites.each do |fav| %>
        <%= render :partial => 'shared/dashboard_card', locals: { recipe: fav.recipe, favorite: fav } %>
      <% end %>
    </div>
  </div>
</div>


