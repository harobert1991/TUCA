<h1 class="text-center">My Cart</h1>
<br>




<% @carts.each do |cart| %>
  <ul class="cart-list">
      <% cart.user_ingredients.each do |user_ingredient| %>
        <li>
          <%= check_box_tag(user_ingredient.dose.ingredient) %>
          <%= user_ingredient.dose.ingredient %>
          <%= link_to "Delete", dose_path, method: :delete %>
          </li>
      <% end %>
    </ul>
<% end %>


